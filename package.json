{
  "name": "@m4tec/penguinplayer",
  "version": "1.2.4",
  "description": "A simple player based on Netease Cloud Music",
  "main": "dist/player.js",
  "scripts": {
    "build": "yarn run webpack --mode=production --env production",
    "watch": "yarn run webpack --watch",
    "watch-ie": "yarn run webpack --watch --env flags=ie",
    "build-dev": "yarn run webpack",
    "vercel-build": "yarn run build && cp -R dist/ demo/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/M4TEC/PenguinPlayer.git"
  },
  "keywords": [
    "blog-plugin",
    "netease",
    "player"
  ],
  "author": "LuoRain",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/M4TEC/PenguinPlayer/issues"
  },
  "homepage": "https://github.com/M4TEC/PenguinPlayer#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "babel-loader": "^8.2.3",
    "colorthief": "^2.3.2",
    "core-js": "^3.19.0",
    "css-loader": "^6.5.0",
    "html-loader": "^3.0.0",
    "ifdef-loader": "^2.3.0",
    "intersection-observer": "^0.12.0",
    "jpeg-js": "^0.4.3",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^6.7.0",
    "pug": "^3.0.2",
    "pug-html-loader": "^1.1.5",
    "raw-loader": "^4.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "smooth-scrollbar": "^8.6.3",
    "stackblur-canvas": "^2.5.0",
    "style-loader": "^3.3.1",
    "svgo": "^2.2.2",
    "svgo-loader": "^3.0.0",
    "vanilla-lazyload": "^17.5.0",
    "webpack": "^5.61.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {}
