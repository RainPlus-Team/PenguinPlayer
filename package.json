{
  "name": "@m4tec/penguinplayer",
  "version": "0.0.1-alpha",
  "description": "A simple player based on Netease Cloud Music",
  "main": "dist/cjs/player.js",
  "module": "dist/esm/player.js",
  "scripts": {
    "build": "yarn run webpack --mode=production --env production",
    "watch": "yarn run webpack --watch",
    "build-dev": "yarn run rollup -c",
    "vercel-build": "yarn run build && cp -R dist/demo public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/M4TEC/PenguinPlayer.git"
  },
  "keywords": [
    "blog-plugin",
    "netease",
    "qq",
    "player"
  ],
  "author": "LuoRain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/M4TEC/PenguinPlayer/issues"
  },
  "homepage": "https://github.com/M4TEC/PenguinPlayer#readme",
  "devDependencies": {
    "@types/preact-i18n": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "core-js": "^3.21.1",
    "preact": "^10.7.1",
    "rollup": "^2.71.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.5.3"
  }
}
